   "source": [
    "def count_vowels(word):\n",
    "    vowels = 'aeiouAEIOU'  \n",
    "    count = 0  \n",
    "    for letter in word:  \n",
    "        if letter in vowels:  \n",
    "            count += 1  \n",
    "    return count  \n",
    "\n",
    "count_vowels(\"elephant\")"
   ]
